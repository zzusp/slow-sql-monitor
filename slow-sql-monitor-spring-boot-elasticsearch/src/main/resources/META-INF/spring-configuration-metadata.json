{
  "groups": [
    {
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "name": "slow-sql-monitor.elasticsearch",
      "type": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties"
    }
  ],
  "properties": [
    {
      "name": "slow-sql-monitor.elasticsearch.enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Enable Elasticsearch.",
      "defaultValue": "true"
    },
    {
      "name": "slow-sql-monitor.elasticsearch.uris",
      "type": "java.util.List<java.lang.String>",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Comma-separated list of the Elasticsearch instances to use.",
      "defaultValue": [
        "http:\/\/localhost:9200"
      ]
    },
    {
      "name": "slow-sql-monitor.elasticsearch.username",
      "type": "java.lang.String",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Username for authentication with Elasticsearch."
    },
    {
      "name": "slow-sql-monitor.elasticsearch.password",
      "type": "java.lang.String",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Password for authentication with Elasticsearch."
    },
    {
      "name": "slow-sql-monitor.elasticsearch.connection-timeout",
      "type": "java.time.Duration",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Connection timeout used when communicating with Elasticsearch.",
      "defaultValue": "1s"
    },
    {
      "name": "slow-sql-monitor.elasticsearch.socket-timeout",
      "type": "java.time.Duration",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Socket timeout used when communicating with Elasticsearch.",
      "defaultValue": "30s"
    },
    {
      "name": "slow-sql-monitor.elasticsearch.path-prefix",
      "type": "java.lang.String",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Prefix added to the path of every request sent to Elasticsearch."
    },
    {
      "name": "slow-sql-monitor.elasticsearch.index-prefix",
      "type": "java.lang.String",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Prefix added to the path of every index from Elasticsearch.",
      "defaultValue": "slow_sql_"
    },
    {
      "name": "slow-sql-monitor.elasticsearch.send-millis",
      "type": "java.lang.Long",
      "sourceType": "com.slowsql.spring.boot.elasticsearch.properties.SlowSqlElasticsearchProperties",
      "description": "Sql execute millis used when send data to Elasticsearch.",
      "defaultValue": "1000"
    }
  ]
}